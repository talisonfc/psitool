    "version": "0.2.0",
    "configurations": [
        {
            "name": "psitool - Web (dev)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port=8080",
                "--web-hostname=localhost",
                "-d",
                "chrome"
            ]
        },
        {
            "name": "psitool (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "psitool (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}